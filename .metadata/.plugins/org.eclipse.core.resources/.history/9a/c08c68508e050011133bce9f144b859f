package castingTask;

public class CastingTask01 {
	public static void main(String[] args) {
		//      6.37, 3.99, 3
		//      3개의 값을 연산하여 3을 만들기
		//      단 모두 한 번씩 사용해야한다.
		float six = 6.37f, threePointNine = 3.99f;
		int three = 3;
		String result = "(int)(%f + %f) / %d = %d"; 
		
		System.out.println(result);
	}
}
